<configure
  xmlns="http://namespaces.zope.org/zope"
  xmlns:browser="http://namespaces.zope.org/browser"
  xmlns:i18n="http://namespaces.zope.org/i18n"
  i18n_domain="launchpad">

  <!-- CveReference -->
  <content class="canonical.launchpad.database.CveReference">
    <allow
      interface="canonical.launchpad.interfaces.ICveReference" />
    <require
      permission="launchpad.AnyPerson"
      set_schema="canonical.launchpad.interfaces.ICveReference"
      />
  </content>

</configure>
