<div class="portlet">

  <h2>Published as</h2>

  <div class="portletBody portletContent">

    <ul tal:condition="context/current_publishings">
      <li class="packages" tal:repeat="pub context/current_publishings">
        <a tal:attributes="href
            pub/distroarchreleasebinarypackagerelease/fmt:url">
          <span tal:replace="pub/distroarchreleasebinarypackagerelease/name">
          pmount
          </span>
          <span tal:replace="pub/distroarchreleasebinarypackagerelease/version">
          1.1
          </span>
          in
          <span tal:replace="pub/distroarchrelease/architecturetag">
            i386
          </span>
          (<span tal:replace="pub/pocket/title">Release</span>)
        </a>
      </li>
    </ul>
    <p tal:condition="not: context/current_publishings">
      <i>Not published at present.</i>
    </p>
  </div>
</div>
