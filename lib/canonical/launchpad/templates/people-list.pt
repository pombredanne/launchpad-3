<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="context/@@main_template/master"
    i18n:domain="launchpad">
  <body>
    <metal:heading fill-slot="pageheading">
      <h1 tal:content="view/header">People/Teams/Ubunteros in Launchpad</h1>
    </metal:heading>

<div metal:fill-slot="main"
     tal:define="bn view/getList">

  <table>
    <tr>
      <td>
        <tal:navigation replace="structure bn/@@+navigation-links-upper" />
      </td>
    </tr>
    <tr tal:repeat="person bn/currentBatch">
      <td>
        <a tal:content="string: ${person/browsername} (${person/name})"
           tal:attributes="href person/fmt:url" />
     </td>
    </tr>
    <tr>
      <td>
        <tal:navigation replace="structure bn/@@+navigation-links-lower" />
      </td>
    </tr>
  </table>

</div>

</body>
</html>
