<metal:page define-macro="master"
            extend-macro="context/@@+shipit-macros/meta-master">

  <metal:block define-slot="top_slot" />
  <head>

    <metal:extra-head fill-slot="extra-head">
      <link rel="icon" href="/@@/ubuntu-icon" />
      <style type="text/css" media="screen"> @import url(/@@/ubuntu-plone.css);</style>
      <style type="text/css" media="screen"> @import url(/@@/ubuntu.css);</style>
      <style type="text/css" media="screen"> @import url(/@@/ubuntu-masthead.css);</style>
      <style type="text/css" media="screen"> @import url(/@@/ubuntu-discs.css);</style>

    </metal:extra-head>

  </head>

  <body>

    <div metal:fill-slot="footer" class="footer inside"
         style="background: url(/@@/ubuntu-header-bg4.png) bottom left repeat-x #fff;">
      &copy; 2004-2007 <a href="http://www.canonical.com">Canonical Ltd</a>.
      Ubuntu, ShipIt and Canonical are registered trademarks of Canonical Ltd.
      <a href="http://www.ubuntu.com/getubuntu/shipit-faq">
        ShipIt FAQ
      </a>
    </div>

  </body>

</metal:page>
