  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  xml:lang="en"
  lang="en"
  dir="ltr"
  metal:use-macro="view/macro:page/applicationhome"
  i18n:domain="launchpad"
>
  <body>
    <h1 metal:fill-slot="heading">Hardware Database Submission</h1>

    <div metal:fill-slot="main">
