<div
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  class="portlet" id="portlet-importstats">

  <h2>Upstream imports</h2>

  <div class="portletBody">
    <div class="portletContent">
	
      <ul>
        <li>Testing: <span tal:replace="view/testing_count">3</span></li>
        <li>Autotested: <span tal:replace="view/autotested_count">3</span></li>
        <li>Test Failed: <span tal:replace="view/testfailed_count">3</span></li>
        <li>Processing: <span tal:replace="view/processing_count">3</span></li>
        <li>Syncing: <span tal:replace="view/syncing_count">3</span></li>
        <li>Stopped: <span tal:replace="view/stopped_count">3</span></li>
        <li>Total: <span tal:replace="view/import_count">23</span></li>
      </ul>
    </div>
  </div>
</div>
