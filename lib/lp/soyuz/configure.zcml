        <allow
            interface="lp.soyuz.interfaces.files.ISourcePackageReleaseFileSet"/>
    </securedutility>

    <!-- ArchiveSigningKey -->

    <class
        class="lp.soyuz.adapters.archivesigningkey.ArchiveSigningKey">
        <allow
            interface="lp.soyuz.interfaces.archivesigningkey.IArchiveSigningKey"/>
    </class>

</configure>
